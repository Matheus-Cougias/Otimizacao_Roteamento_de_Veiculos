     let nosTotais := nosTotais + if nosIteracao == 0 then 1 else nosIteracao;
     let iteracao := iteracao + 1;
     let lb := tour;
     let{(i,j) in Ac: x[i,j] > 0.9} NEXT[i] := j;
      let V := N;
